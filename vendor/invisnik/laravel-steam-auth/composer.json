{
    "name": "invisnik/laravel-steam-auth",
    "description": "Laravel Steam Auth",
    "keywords": ["steam", "laravel", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Brytkov",
            "email": "invis.nik@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/http": "5.*",
        "illuminate/routing": "5.*",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Invisnik\\LaravelSteamAuth\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Invisnik\\LaravelSteamAuth\\SteamServiceProvider"
            ]
        }
    }
}
